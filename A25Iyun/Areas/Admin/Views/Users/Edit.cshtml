@model Users
@{
    ViewData["Title"] = "Edit";
}

<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <input type="text" asp-for="@Model.Name" class="form-control" placeholder="Enter Name">
    </div>
     <div class="form-group">
        <input type="text" asp-for="@Model.Description" class="form-control" placeholder="Description Name">
    </div>
     <div class="form-group">
        <input type="number" asp-for="@Model.Raiting" class="form-control" placeholder="Raiting">
    </div>
     <div class="form-group">
        <input type="file" asp-for="@Model.Image" class="form-control" >
    </div>
   
    <button type="submit" class="btn btn-success">Submit</button>
</form>
